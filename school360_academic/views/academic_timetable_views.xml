<odoo>
    <record id="view_academic_timetable_list" model="ir.ui.view">
        <field name="name">academic.timetable.list</field>
        <field name="model">school360_academic.timetable</field>
        <field name="arch" type="xml">
            <list>
                <field name="day_of_week"/>
                <field name="start_time" widget="float_time"/>
                <field name="end_time" widget="float_time"/>
                <field name="section_id"/>
                <field name="subject_id"/>
                <field name="teacher_id"/>
            </list>
        </field>
    </record>

    <record id="action_academic_timetable" model="ir.actions.act_window">
        <field name="name">Timetable</field>
        <field name="res_model">school360_academic.timetable</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>